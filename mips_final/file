
ori  $10, $0, 0X00 ; inicia el valor
ori  $11, $0, 0X01 ; inicia el limite
ori  $12, $0, 0X02 ; inicia el limite
ori  $13, $0, 0X03 ; inicia el limite
loop:
	addi $10,  $10, 1
	beq  $10, $11, p1
	beq  $10, $12, p2
	beq  $10, $13, p3

p1:
j loop
p2:
jal loop
p3:
jr $31
p4:
ori  $9, $0, 0X04 ; direccion de salto
jalr $9
skip:
ori  $1, $0, 0xffff ; finish

halt

end


